"xbacklight -inc 10"
	XF86MonBrightnessUp
"xbacklight -dec 10"
	XF86MonBrightnessDown
"pactl set-sink-volume alsa_output.pci-0000_00_1b.0.analog-stereo +10%"
	XF86AudioRaiseVolume
"pactl set-sink-volume alsa_output.pci-0000_00_1b.0.analog-stereo -10%"
	XF86AudioLowerVolume
"pactl set-sink-mute alsa_output.pci-0000_00_1b.0.analog-stereo toggle"
	XF86AudioMute
